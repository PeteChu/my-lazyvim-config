{
  "mcpServers": {
    "code-reasoning": {
      "args": [
        "-y",
        "@mettamatt/code-reasoning"
      ],
      "autoApprove": [
        "code-reasoning"
      ],
      "command": "npx",
      "disabled": true
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "command": "npx",
      "disabled": true
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx",
      "disabled": true,
      "name": "sequentialthinking"
    },
    "tidewave": {
      "args": [
        "http://localhost:4000/tidewave/mcp"
      ],
      "autoApprove": [
        "get_logs",
        "get_source_location",
        "get_package_location",
        "project_eval",
        "execute_sql_query",
        "get_ecto_schemas",
        "get_process_info",
        "trace_process",
        "list_liveview_pages",
        "package_docs_search",
        "package_search"
      ],
      "command": "mcp-proxy",
      "disabled": true
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true,
      "disabled_tools": []
    },
    "neovim": {
      "custom_instructions": {
        "disabled": false
      },
      "disabled": true,
      "disabled_prompts": []
    }
  }
}
